    <script>  //numeros
        let n=[20,19,17,18,30,68,1];
        let n2=[7,13,39,40,90,21];
        function divisor(n,n2){
           for(i=0;i<n.length;i++){
              if(n[i]%2==0){
                 console.log("es divisible por 2 el:" +n[i])
              }else{
                 console.log("no es divisible por 2 el:" +n[i])
                 for(a=0;a<n2.length;a++){
                    if(n2[a]%11==0){
                       console.log("si es divisible por 11")          
                    
                    }
                 }
        
              }
           }
        }
        divisor(n, n2)
        
                </script>
